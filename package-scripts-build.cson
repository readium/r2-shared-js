  'prebuild:#ECMASCRIPT#': '
    npm run lint &&
    npm run clean
  '

  'build:#ECMASCRIPT#': '
    npm run transpile:typescript:#ECMASCRIPT#
  '

  'postbuild:#ECMASCRIPT#': '
    npm run afterdist
  '

  'transpile:typescript:#ECMASCRIPT#': '
    tsc --pretty -p "./tsconfigs/tsconfig-#ECMASCRIPT#-all.json"
  '
